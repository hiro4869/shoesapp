<h1>お問い合わせ</h1>
<%= form_for @inquiry do |f| %>
  <p>タイトル</p>
  <p><%= f.text_field :title %></p>
  <br>
  <p>氏名</p>
  <p><%= f.text_field :name %></p>
  <br>
  <p>Eメール</p>
  <p><%= f.text_field :email %></p>
  <br>
  <p>お問い合わせ内容</p>
  <p><%= f.text_area :body %></p>
  <br>
  <p><%= f.submit "　送信する　", class: "btn btn-primary btn-lg" %></p>
<% end %>

